<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
<display-name>Tracking-QA</display-name>
  <welcome-file-list>
    <welcome-file>readme.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>trackingServlet</servlet-name>
    <servlet-class>com.servlet.trackingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>madhouseServlet</servlet-name>
    <servlet-class>com.servlet.madhouseServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>trackingServlet</servlet-name>
    <url-pattern>/trackingCount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>trackingServlet</servlet-name>
    <url-pattern>/clkCount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>trackingServlet</servlet-name>
    <url-pattern>/impCount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>trackingServlet</servlet-name>
    <url-pattern>/imptrackingCount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>trackingServlet</servlet-name>
    <url-pattern>/clktrackingCount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>madhouseServlet</servlet-name>
    <url-pattern>/madhouse-protobuf</url-pattern>
  </servlet-mapping>

</web-app>